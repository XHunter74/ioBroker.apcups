{
    "i18n": true,
    "type": "tabs",
    "tabsStyle": {
        "width": "calc(100% - 100px)"
    },
    "items": {
        "_options": {
            "type": "panel",
            "label": "Main settings",
            "items": {
                "upsList": {
                    "type": "table",
                    "label": "upsList",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12,
                    "items": [
                        {
                            "type": "text",
                            "attr": "upsIp",
                            "width": "40%",
                            "title": "upsIp",
                            "tooltip": "enterUpsIp",
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "number",
                            "attr": "upsPort",
                            "width": "20%",
                            "title": "upsPort",
                            "tooltip": "enterUpsPort",
                            "filter": false,
                            "sort": false,
                            "min": 1054,
                            "step": 1,
                            "default": 3551
                        }
                    ]
                },
                "pollingInterval": {
                    "type": "number",
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 2,
                    "label": "pollingInterval",
                    "newLine": true,
                    "min": 1000,
                    "max": 60000
                }
            }
        }
    }
}